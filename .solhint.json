{
    "extends": "solhint:recommended",
    "rules": {
      "compiler-version": ["error", "^0.8.9"],
      "func-visibility": ["warn", { "ignoreConstructors": true }],
      "reason-string": [false],
      "contract-name-camelcase": [false],
      "state-visibility": [false],
      "no-unused-vars": [false],
      "no-empty-blocks": [false]
    }
  }